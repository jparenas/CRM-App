{
  "name": "crm-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run build && npm run start",
    "build": "tsc -p tsconfig-build.json && npm run copy:public",
    "lint": "vue-cli-service lint client common server",
    "build:prod": "cross-env NODE_ENV=production tsc -p tsconfig-build.json && vue-cli-service build --modern && npm run copy:public",
    "clean": "rimraf dist",
    "copy:public": "mkdirp dist/server/public && copyfiles -u 2 -a \"server/public/**/*\" dist/server/public",
    "dev": "npm run clean && cross-env NODE_ENV=development nodemon --exec \"npm rum serve\"",
    "knex": "tsc -p tsconfig-build.json && knex --knexfile ./dist/server/src/remote/knexfile.js",
    "migrate:latest": "npm run knex -- migrate:latest",
    "migrate:make": "npm run knex -- migrate:make",
    "migrate:rollback": "npm run knex -- migrate:rollback",
    "seed:make": "npm run knex -- seed:make",
    "seed:run": "npm run knex -- seed:run",
    "start": "node ./dist/server/src/main.js",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "argon2": "^0.24.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.5",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-jwt": "^5.3.1",
    "helmet": "^3.21.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.4",
    "module-alias": "^2.2.1",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.6.2",
    "vue-server-renderer": "^2.6.10"
  },
  "devDependencies": {
    "@lifeomic/axios-fetch": "^1.4.1",
    "@mdi/font": "^4.4.95",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-jwt": "0.0.42",
    "@types/faker": "^4.1.5",
    "@types/graphql-iso-date": "^3.3.3",
    "@types/helmet": "0.0.44",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/lodash": "^4.14.138",
    "@types/morgan": "^1.7.37",
    "@types/source-map": "^0.5.7",
    "@types/vuelidate": "^0.7.7",
    "@types/webpack": "^4.39.1",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-hot-middleware": "^2.16.5",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-pwa": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.21.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.1",
    "eslint": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "faker": "^4.1.0",
    "graphql": "^14.5.6",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sass": "^1.22.12",
    "sass-loader": "^7.3.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.2",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-rc.5",
    "vue-class-component": "^7.1.0",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.4.0",
    "vue-cookies": "^1.5.13",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "vuelidate": "^0.7.4",
    "vuetify": "^2.0.17",
    "vuetify-loader": "^1.3.0",
    "vuex": "^3.1.1",
    "webpack-bundle-analyzer": "^3.5.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "_moduleAliases": {
    "@c": "./dist/common/src/.",
    "@s": "./dist/server/src/.",
    "@root": "./."
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ext": "js ts json",
    "watch": [
      "."
    ],
    "ignore": [
      "client/",
      "dist/",
      "**/migrations/"
    ]
  }
}
